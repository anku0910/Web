<script language="JavaScript"> 
function MM_showHideLayers() { //v9.0
  var i,p,v,obj,args=MM_showHideLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) 
  with (document) if (getElementById && ((obj=getElementById(args[i]))!=null)) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'visible':(v=='hide')?'hidden':v; }
    obj.visibility=v; }
}

function answered(ansid,ans_val){
 var ans_corr = ansid + "_correct"
 var ans_user = ansid + "_user"
 var ans_apx = ansid + "_apx"
 var ans_apm = ansid + "_apm"
 var ans_cbox = ansid + "_cbox"
 
   document.getElementById(ans_user).value = ans_val ;
   correctans = document.getElementById(ans_corr).value;
   
   if (correctans == ans_val){
      MM_showHideLayers(ans_apx,'','hide');
      MM_showHideLayers(ans_apm,'','hide');	  
   }else{
      MM_showHideLayers(ans_apx,'','show');
      MM_showHideLayers(ans_apm,'','show');
      document.getElementById(ans_cbox).checked = true	  
   }
}

function answered_multiple(ansname,ss){
 var ans_corr = ansname + "_correct"
 var ans_user = ansname + "_user"
 var ans_apx = ansname + "_apx"
 var ans_apm = ansname + "_apm"
 var ans_cbox = ansname + "_cbox"

  var ansid = ansname + "d" ;
  var ans_u = ansname + "u" ;
  var ans_checkbox = ss ;
  var opt = '';

   correctans = document.getElementById(ans_corr).value;
  
  for (var i = 1 ; i <= 4 ; i++) {
   if (eval("document.forms['subj']."+ "ans" + ans_checkbox + "_" + i +".checked")) {
      opt += i ;
   }
  }

//alert(document.getElementById(ans_cbox).checked)  
   if (correctans == opt){
      MM_showHideLayers(ans_apx,'','hide');
      MM_showHideLayers(ans_apm,'','hide');	  
      document.getElementById(ans_cbox).checked = false;	  	  
   }else{
      MM_showHideLayers(ans_apx,'','show');
      MM_showHideLayers(ans_apm,'','show');
      document.getElementById(ans_cbox).checked = true;
   }  
  if (opt == "") {
     document.getElementById(ans_cbox).checked = false;	 
     MM_showHideLayers(ans_apx,'','hide');	 
     document.getElementById(ans_user).value = "";
  }
  else
  {
     document.getElementById(ans_user).value = opt ;
  } 
}


